{
  "coc.preferences.formatOnSaveFiletypes": ["json"],
  "explorer.quitOnOpen": false,
  "explorer.position": "left",
  "explorer.file.showHiddenFiles": true,
  "explorer.file.hiddenRules": {
    "filenames": [".git"]
  },
  "explorer.git.enable": false,
  "explorer.keyMappings.global": {
    "ad": "addDirectory",
    "af": "addFile",
    "dd": "delete",
    "dc": "cutFile",
    "f": "search",
    "F": "searchRecursive",
    "gb": "gotoSource:buffer",
    "gf": "gotoSource:file",
    "gj": "expandableNext",
    "gk": "expandablePrev",
    "gl": [
      "expandable?",
      ["expanded?", "cd", "expand"],
      ["expandablePrev", "cd"]
    ],
    "gh": ["gotoParent"],
    "h": ["expandable?", ["expanded?", "collapse", "gotoParent"], "gotoParent"],
    "l": ["expandable?", ["expanded?", "cd", "expand"], "open"],
    "<C-h>": "gotoParent",
    "<C-l>": "open",
    "os": "open:split",
    "ot": "open:tab",
    "ov": "open:vsplit",
    "q": "quit",
    "R": "refresh",
    "r": "rename",
    "yn": "copyFilename",
    "yp": "copyFilepath",
    "yy": "copyFile",
    "zc": "collapse",
    "zC": "collapse:all",
    "zh": "toggleHidden",
    "zo": "expand",
    "zO": "expand:recursive",
    "!": "systemExecute",
    "?": "help",
    "<bs><bs>": "gotoParent",
    "<esc>": "esc",
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "<space>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "explorer.presets": {},
  "explorer.root.strategies": ["sourceBuffer"],
  "explorer.width": 30,
  "java.project.referencedLibraries": [
    "/home/andre/.local/share/algs4/algs4.jar"
  ],
  "suggest.detailField": "preview",
  "suggest.maxCompleteItemCount": 20,
  "suggest.snippetsSupport": false
}
